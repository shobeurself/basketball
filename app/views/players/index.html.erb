<h1>Listing Players</h1>
 
<table>
  <tr>
    <th>Name</th>
    <th>Team</th>
    <th colspan="3"></th>
  </tr>
 
  <% @players.each do |player| %>
    <tr>
      <td><%= player.name %></td>
      <td><%= player.team %></td>
      <td><%= link_to 'Show', player_path(player) %></td>
      <td><%= link_to 'Edit', edit_player_path(player) %></td>
    </tr>
  <% end %>
</table>